{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "allowUnreachableCode": false,
        "alwaysStrict": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "isolatedModules": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "pretty": true,
        "strictPropertyInitialization": true,
        "strictNullChecks": true,
        "strict": true,
        "strictFunctionTypes": true,
        "allowJs": true,
        "outDir": "./dist",
        "lib": [
            "es2015",
            "es2016",
            "es2017",
            "es2018",
            "esnext.asynciterable"
        ],
        "baseUrl": ".",
    },
    "exclude": [
        "node_modules"
    ]
}